<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane fx:id="gob_rootPane" prefWidth="550.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.ChangeIpPortController">
    <children>
        <HBox alignment="CENTER_LEFT" prefHeight="400.0" prefWidth="550.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <children>
                  <ListView fx:id="gob_lvOptions" onMouseClicked="#loadView" prefHeight="400.0" prefWidth="209.0" />
            <VBox alignment="CENTER" prefHeight="400.0" prefWidth="454.0" spacing="20.0">
               <children>
                  <Label text="New Server-IP" />
                  <TextField fx:id="tfServerIp">
                     <VBox.margin>
                        <Insets left="50.0" right="50.0" />
                     </VBox.margin>
                  </TextField>
                  <Label text="New Port" />
                  <TextField fx:id="tfPort" prefHeight="31.0" prefWidth="307.0">
                     <VBox.margin>
                        <Insets left="150.0" right="150.0" />
                     </VBox.margin>
                  </TextField>
                  <Button fx:id="btnConnect" mnemonicParsing="false" text="Connect" />
               </children>
            </VBox>
            </children>
        </HBox>
    </children>
</AnchorPane>
