<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.control.TreeView?>
<?import javafx.scene.layout.StackPane?>

<SplitPane dividerPositions="0.5" prefHeight="600.0" prefWidth="800.0" fx:controller="controller.MainController" xmlns="http://javafx.com/javafx/8.0.112" xmlns:fx="http://javafx.com/fxml/1">
    <items>
            <StackPane prefHeight="150.0" prefWidth="200.0">
                  <children>
                       <TreeView />
                   </children>
            </StackPane>
            <StackPane alignment="TOP_RIGHT" prefHeight="150.0" prefWidth="200.0">
                   <children>
                       <Button id="btnSettings" fx:id="btnSettings" mnemonicParsing="false" onAction="#onClick" text="Settings">
               <StackPane.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </StackPane.margin></Button>
                   </children>
            </StackPane>
    </items>
</SplitPane>
