<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane fx:id="gob_rootPane" prefWidth="550.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.AdminLoginController">
   <children>
      <VBox alignment="CENTER" prefHeight="400.0" prefWidth="550.0" spacing="20.0">
         <children>
            <Label text="Benutzername" />
            <TextField fx:id="tfUserName">
               <VBox.margin>
                  <Insets left="150.0" right="150.0" />
               </VBox.margin>
            </TextField>
            <Label text="Passwort" />
            <PasswordField fx:id="pwPasswort">
               <VBox.margin>
                  <Insets left="150.0" right="150.0" />
               </VBox.margin>
            </PasswordField>
            <Button fx:id="btnLogin" mnemonicParsing="false" onAction="#onClick" text="Login" />
         </children>
      </VBox>
   </children>
</AnchorPane>
