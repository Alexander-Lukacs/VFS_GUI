<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.*?>
<AnchorPane fx:id="gob_rootPane" prefWidth="550.0" xmlns="http://javafx.com/javafx/8"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.AddAdminController">
    <HBox alignment="CENTER_LEFT" prefHeight="400.0" prefWidth="550.0" AnchorPane.bottomAnchor="0.0"
          AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <ListView fx:id="gob_lvOptions" onMouseClicked="#loadView" prefHeight="400.0" prefWidth="209.0"/>
        <VBox alignment="CENTER" prefHeight="400.0" prefWidth="454.0" spacing="20.0">
            <Label text="Enter Username"/>
            <TextField fx:id="tfAddAdmin">
                <VBox.margin>
                    <Insets left="50.0" right="50.0"/>
                </VBox.margin>
            </TextField>
            <Button mnemonicParsing="false" onAction="#onClick" text="Add"/>
        </VBox>
    </HBox>
</AnchorPane>
