<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane fx:id="gob_rootPane" prefWidth="550.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.ChangePWController">
    <children>
          <HBox alignment="CENTER_LEFT" prefHeight="400.0" prefWidth="550.0">
              <children>
                  <ListView fx:id="gob_lvOptions" onMouseClicked="#loadView" prefHeight="400.0" prefWidth="209.0" />
                  <VBox alignment="TOP_CENTER" prefWidth="454.0" spacing="20.0">
                      <children>
                          <Label alignment="CENTER" text="Old Password">
                              <VBox.margin>
                                  <Insets top="40.0" />
                              </VBox.margin>
                          </Label>
                          <PasswordField fx:id="txtOldPassword" promptText=" ">
                              <VBox.margin>
                                  <Insets left="100.0" right="100.0" />
                              </VBox.margin>
                          </PasswordField>
                          <Label alignment="CENTER" text="New Password" />
                          <PasswordField fx:id="txtNewPassword" layoutX="110.0" layoutY="87.0" promptText=" ">
                              <VBox.margin>
                                  <Insets left="100.0" right="100.0" />
                              </VBox.margin>
                          </PasswordField>
                          <Label alignment="CENTER" text="Confirm Password" />
                          <PasswordField fx:id="txtConfirmPassword" layoutX="10.0" layoutY="169.0" promptText=" ">
                              <VBox.margin>
                                  <Insets left="100.0" right="100.0" />
                              </VBox.margin>
                          </PasswordField>
                          <Button fx:id="gob_btnSave" mnemonicParsing="false" text="Save" />
                      </children>
                  </VBox>
              </children>
          </HBox>
    </children>
</AnchorPane>
